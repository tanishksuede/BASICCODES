class circle:
    def __init__(self, radius):
        self.radius = radius

    def area(self):
        return 3.14 * self.radius * self.radius
    
    def circumference(self):
        return 2 * 3.14 * self.radius
        return self.radius
    
z = circle(int(input("Enter the radius of the circle: ")))
print("Area of circle:", z.area())
print("Circumference of circle:", z.circumference())
